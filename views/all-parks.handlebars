  <!-- parks a to z -->
<div class="header">
        <h1> Parks A - Z </h1>
        <h2>Select a park for more information</h2>
</div>

<ul class="pure-g">
    {{#each parks}}
    <div class="pure-u-4-5 a-z">
        <a href="/{{parkName}}"><h2>{{parkName}}</h2></a>
        <h6>{{address}}</h6>
        <h6>Size: {{size}}</h6>
    {{!-- partial for icon list --}}
        {{> parks/park-block}}
        
    </div>           
    {{/each}}
</ul>