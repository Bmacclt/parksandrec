  <!-- parks a to z -->
<div class="header">
        <h1> Parks A - Z </h1>
        <h2>Select a park for more information</h2>
</div>

<ul class="pure-g">
    {{#each parks}}
    <div class="pure-u-4-5">
        <h2>{{parkName}}</h2>
        <h4>Acres: {{size}}</h4>
    <ul class="icon-list">
        {{#if restrooms}} 
            <li><a href="#">
                <img src="./assets/images/png/restrooms.png" class="pure-img icon" alt="Restrooms available">
            </a></li>
        {{/if}}{{#if handiAcces}}
            <li><a href="#">
                <img src="./assets/images/png/wheelchair-access.png" class="pure-img icon" alt="Wheel Chair Accessible">
            </a></li>
        {{/if}}
    </ul>    
        
    </div>           
    {{/each}}
</ul>